<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

<changeSet id="create_relation_alarme_medicamento" author="wagner.junior">
		<createTable tableName="alarme_medicamento">
			<column name="id" type="${TYPE_ID}">
				<constraints nullable="false" primaryKey="true" primaryKeyName="PK_ALARME_MEDICAMENTO"/>
			</column>
			<column name="id_alarme" type="${TYPE_ID}">
				<constraints nullable="false" unique="false" foreignKeyName="FK_ALARMEMEDICAMENTO_ALARME" references="alarme(id)"/>
			</column>
			<column name="id_medicamento" type="${TYPE_ID}">
				<constraints nullable="false" unique="false" foreignKeyName="FK_ALARMEMEDICAMENTO_MEDICAMENTO" references="medicamento(id_medicamento)"/>
			</column>
		</createTable>
	                                                 
	</changeSet>
</databaseChangeLog>