<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create_table_sexo" author="douglas.mendonca">
		<createTable tableName="sexo">
			<column name="id_sexo" type="${TYPE_ID}">
				<constraints nullable="false" primaryKey="true"
					primaryKeyName="PK_SEXO" />
			</column>
			<column name="sexo" type="${TYPE_DESCRICAO}">
				<constraints nullable="false" unique="true" />
			</column>
		</createTable>
	</changeSet>

	<changeSet id="create_table_sexo_aud"
		author="douglas.mendonca">
		<createTable tableName="sexo_aud">
			<column name="id_sexo" type="${TYPE_ID}">
				<constraints nullable="false" />
			</column>
			<column name="sexo" type="${TYPE_DESCRICAO}">
				<constraints nullable="false" unique="true" />
			</column>
			<column name="rev" type="${TYPE_NUMBER}">
				<constraints nullable="false"
					referencedTableName="revinfo" referencedColumnNames="rev"
					foreignKeyName="sexo_revinfo_fk" />
			</column>
			<column name="revtype" type="${TYPE_NUMBER}">
				<constraints nullable="false" />
			</column>
		</createTable>
		<addPrimaryKey constraintName="sexo_aud_pkey"
			tableName="sexo_aud" columnNames="id_sexo, rev" />
	</changeSet>

</databaseChangeLog>