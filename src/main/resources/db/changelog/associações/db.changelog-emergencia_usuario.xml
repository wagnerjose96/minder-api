<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create_relation_emergencia_usuario" author="wagner.junior">
		<createTable tableName="emergencia_usuario">
			<column name="id" type="${TYPE_ID}">
				<constraints nullable="false" primaryKey="true"
					primaryKeyName="PK_EMERGENCIA_USUARIO" />
			</column>
			<column name="id_emergencia" type="${TYPE_ID}">
				<constraints nullable="false" unique="false"
					foreignKeyName="FK_EMERGENCIAUSUARIO_EMERGENCIA" references="emergencia(id_emergencia)" />
			</column>
			<column name="id_usuario" type="${TYPE_ID}">
				<constraints nullable="false" unique="false"
					foreignKeyName="FK_EMERGENCIAUSUARIO_USUARIO" references="usuario(id)" />
			</column>
		</createTable>

	</changeSet>
</databaseChangeLog>