<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create_relation_contato_telefone" author="camila.morais">
		<createTable tableName="contato_telefone">
			<column name="id" type="${TYPE_ID}">
				<constraints nullable="false" primaryKey="true"
					primaryKeyName="PK_CONTATO_TELEFONE" />
			</column>
			<column name="id_contato" type="${TYPE_ID}">
				<constraints nullable="false" unique="false"
					foreignKeyName="FK_CONTATOTELEFONE_CONTATO" references="contato(id)" />
			</column>
			<column name="id_telefone" type="${TYPE_ID}">
				<constraints nullable="false" unique="false"
					foreignKeyName="FK_CONTATOTELEFONE_TELEFONE" references="telefone(id_telefone)" />
			</column>
		</createTable>

	</changeSet>
</databaseChangeLog>