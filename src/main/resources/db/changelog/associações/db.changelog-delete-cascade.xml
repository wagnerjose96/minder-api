<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="addForeignKeyConstraint-alarme" author="wagner.junior">
		<addForeignKeyConstraint baseColumnNames="id"
			baseTableName="alarme" constraintName="FK_ALARMEMEDICAMENTO_ALARME_DELETE"
			deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
			onUpdate="RESTRICT" referencedColumnNames="id"
			referencedTableName="alarme_medicamento" />
	</changeSet>
	
	<changeSet id="addForeignKeyConstraint-contato-associacao" author="wagner.junior">
		<addForeignKeyConstraint baseColumnNames="id"
			baseTableName="contato" constraintName="FK_CONTATOTELEFONE_DELETE"
			deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
			onUpdate="RESTRICT" referencedColumnNames="id"
			referencedTableName="contato_telefone" />
	</changeSet>
	
	<changeSet id="addForeignKeyConstraint-contato-telefone" author="wagner.junior">
		<addForeignKeyConstraint baseColumnNames="id"
			baseTableName="contato" constraintName="FK_TELEFONE_DELETE"
			deferrable="true" initiallyDeferred="true" onDelete="CASCADE"
			onUpdate="RESTRICT" referencedColumnNames="id_telefone"
			referencedTableName="telefone" />
	</changeSet>
</databaseChangeLog>