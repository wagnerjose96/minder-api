<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create_table_tipo_sanguineo" author="douglas.mendonca">
		<createTable tableName="tipo_sanguineo">
			<column name="id_tipo_sanguineo" type="${TYPE_ID}">
				<constraints nullable="false" primaryKey="true" primaryKeyName="PK_TIPO_SANGUINEO"/>
			</column>
			<column name="tipo_sangue" type="${TYPE_DESCRICAO}">
				<constraints nullable="false" />
			</column>	
			<column name="rh" type="${TYPE_DESCRICAO}">
				<constraints nullable="false" />
			</column>
		</createTable>
		
	</changeSet>
	
	<changeSet id="create_table_tipo_sanguineo_aud" author="douglas.mendonca">
		<createTable tableName="tipo_sanguineo_aud">
			<column name="id_tipo_sanguineo" type="${TYPE_ID}">
				<constraints nullable="false" />
			</column>
			<column name="tipo_sangue" type="${TYPE_DESCRICAO}">
				<constraints nullable="false" />
			</column>	
			<column name="rh" type="${TYPE_DESCRICAO}">
				<constraints nullable="false" />
			</column>
			<column name="rev" type="${TYPE_NUMBER}">
				<constraints nullable="false" referencedTableName="revinfo"
					referencedColumnNames="rev" foreignKeyName="FK_TIPO_SANGUINEO_REVINFO" />
			</column>
			<column name="revtype" type="${TYPE_NUMBER}">
				<constraints nullable="false" />
			</column>
			</createTable>
			<addPrimaryKey constraintName="PK_TIPO_SANGUINEO_AUD"
				tableName="tipo_sanguineo_aud" columnNames="id_tipo_sanguineo, rev" />	
		
	</changeSet>
	
	
	</databaseChangeLog>